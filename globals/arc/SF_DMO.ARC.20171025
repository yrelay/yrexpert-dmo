$ALLER.A("FIN")
:
$ADRESSE("QUESTIONNEMENT")
%REP(1)="NR" ET %REP(1)=$MESSAGE("Bonjour "_%NOM.FAMILLE_" que voulez-vous savoir ? ")
$FIN
:
$ADRESSE("FIN")
$FIN

